    <table summary="section form fields">
        <tr>
            <th>Name</th>
            <td><%= f.text_field(:name) %></td>
        </tr>
        <tr>
            <th>Position</th>
            <td><%= f.select(:position, 1..@subject_count) %></td>
        </tr>
        <tr>
            <th>Created</th>
            <td><%= f.datetime_select(:created_at, :order => [:month, :day, :year]) %></td>
        </tr>
    </table>